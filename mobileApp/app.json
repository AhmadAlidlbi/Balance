{
  "expo": {
    "name": "Balance",
    "slug": "Balance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ahmedidlbi.Balance"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",{
          "fonts":["./src/assets/fonts/Poppins-Medium.ttf"
        ]
        }
      ]
     ,
      "expo-secure-store"
      
    ],
    "extra": {
      "eas": {
        "projectId": "0bf01b67-76b4-4a2d-8623-1f1331ff3183"
      }
    }
  }
}
